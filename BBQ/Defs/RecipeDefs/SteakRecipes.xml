<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<RecipeDef ParentName="CookMealBase">
    <defName>CookMealSteak</defName>
    <label>cook steak</label>
    <description>Cook a very complex meal from meat. None of the ingredients are wasted.</description>
    <workAmount>400</workAmount>
    <jobString>Cooking steak.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>0.5</count>
      </li>
    </ingredients>
    <products>
      <MealSteak>1</MealSteak>
    </products>
    <skillRequirements>
      <Cooking>8</Cooking>
    </skillRequirements>
  </RecipeDef>

  <RecipeDef ParentName="CookMealBase">
    <defName>CookMealSteakBulk</defName>
    <label>cook steak x4</label>
    <description>Cook 4 very complex meals from meat. Much of the ingredients are wasted.</description>
    <jobString>Cooking 4 steaks.</jobString>
    <workAmount>1600</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>2.0</count>
      </li>
    </ingredients>
    <products>
      <MealSteak>4</MealSteak>
    </products>
    <skillRequirements>
      <Cooking>8</Cooking>
    </skillRequirements>
  </RecipeDef>
</Defs>